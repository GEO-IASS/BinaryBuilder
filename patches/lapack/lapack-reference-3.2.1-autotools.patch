-	( cd TESTING ; rm -f xlintst* ; $(MAKE)  LAPACKLIB='SRC/VARIANTS/LIB/choltop.a ../../$(LAPACKLIB)' ; \
-	mv stest.out stest_choltop.out ; mv dtest.out dtest_choltop.out ; mv ctest.out ctest_choltop.out ; mv ztest.out ztest_choltop.out )
